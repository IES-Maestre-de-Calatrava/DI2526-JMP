<Window x:Class="Contenedor_Canvas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Contenedor_Canvas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas>
            <Button Content="Top Izquierda" Canvas.Top="10" Canvas.Left="10"/>
            <Button Content="Top Derecha" Canvas.Top="10" Canvas.Left="700"/>
            <Button Content="Bottom izquieda" Canvas.Top="400" Canvas.Left="10"/>
            <Button Content="Botton Derecha" Canvas.Top="400" Canvas.Left="700"/>
        </Canvas>

        <Canvas>
            <Ellipse Panel.ZIndex="2" Fill="Gainsboro" Canvas.Left="25" Canvas.Top="25" Width="200" Height="200"/>
            <Rectangle Panel.ZIndex="3" Fill="LightBlue" Canvas.Left="25" Canvas.Top="50" Width="50" Height="50"/>
            <Rectangle Panel.ZIndex="2" Fill="LightCoral" Canvas.Left="50" Canvas.Top="75" Width="50" Height="50"/>
            <Rectangle Panel.ZIndex="4" Fill="LightCyan" Canvas.Left="75" Canvas.Top="100" Width="50" Height="50"/>
        </Canvas>
    </Grid>
</Window>
