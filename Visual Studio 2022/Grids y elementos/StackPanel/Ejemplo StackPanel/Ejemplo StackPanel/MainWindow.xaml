<Window x:Class="Ejemplo_StackPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ejemplo_StackPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="120" Width="250">
    <StackPanel Name="contenedor" Orientation="Vertical">
        <Button Content="BTN 1 1"/>
        <Button Content="BTN 0 2"/>
        <Label Content="Label"/>
        <TextBox Text="TextBox" TextWrapping="Wrap"/>
        <RadioButton Content="RadioButton"/>


    </StackPanel>
</Window>
